{
  "name": "fushuma-governance-hub",
  "version": "1.0.0",
  "description": "The nexus for community interaction, governance, and economic activity in the Fushuma ecosystem",
  "type": "module",
  "scripts": {
    "dev": "tsx server/_core/index.ts",
    "dev:api": "tsx server/_core/index.ts",
    "dev:indexer": "tsx server/services/indexer/index.ts",
    "dev:rates": "tsx server/services/rates/index.ts",
    "build": "vite build",
    "build:full": "vite build && tsc --project tsconfig.server.json",
    "preview": "NODE_ENV=production tsx server/_core/index.ts",
    "start": "NODE_ENV=production tsx server/_core/index.ts",
    "start:all": "pm2 start ecosystem.config.js --env production",
    "stop:all": "pm2 stop ecosystem.config.js",
    "restart:all": "pm2 reload ecosystem.config.js",
    "logs": "pm2 logs",
    "status": "pm2 status",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "backup": "tsx scripts/backup.ts",
    "restore": "tsx scripts/restore.ts",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
